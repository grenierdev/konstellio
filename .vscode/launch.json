			"type": "node",
			"request": "launch",
			"name": "Test @konstellio/odm",
			"program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
			"args": [
				"-u",
				"tdd",
				"--timeout",
				"999999",
				"--colors",
				"--require",
				"ts-node/register",
				"--full-trace",
				"${workspaceFolder}/packages/konstellio/odm/test/**/*.spec.ts",
				"--exit"
			],
			"internalConsoleOptions": "openOnSessionStart"
		},
